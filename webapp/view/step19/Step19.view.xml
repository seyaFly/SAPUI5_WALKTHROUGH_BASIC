<mvc:View
	controllerName="onem.seya.walkthrough.controller.step19.Step19"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
	<Page
		showNavButton="true"
		navButtonPress="navBackHandler"
		class="myAppDemoWT"
	>
		<headerContent>
			<Button
				icon="sap-icon://hello-world"
				press="onOpenDialog"
			/>
		</headerContent>
		<content>
			<mvc:XMLView
				viewName="onem.seya.walkthrough.view.step19.nest.HelloPanel"
				async="true"
			/>
		</content>
	</Page>
</mvc:View>