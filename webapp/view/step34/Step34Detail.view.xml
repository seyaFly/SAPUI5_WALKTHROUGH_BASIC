<mvc:View
	controllerName="onem.seya.walkthrough.controller.step34.Step34Detail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
	xmlns:wt="onem.seya.walkthrough.control"
>
	<Page
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress=".onNavBack"
	>
		<ObjectHeader
			intro="{invoice>ShipperName}"
			title="{invoice>ProductName}"
		/>
		<wt:ProductRating
			id="rating"
			class="sapUiSmallMarginBeginEnd"
			change=".onRatingChange"
		/>
	</Page>
</mvc:View>